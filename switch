import java.util.Scanner;
public class BasicCalculator{
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        String sign = sc.next();
        int b = sc.nextInt();
        
        //put your logic here
        switch(sign){
            case "+": System.out.println(a+b); break;
            case "-": System.out.println(a-b); break;
            case "*": System.out.println(a*b); break;
            case "/":System.out.println(a/b); break;
            
        }
    }
}
